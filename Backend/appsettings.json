{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Tunnel": {
    "Url": "https://apireverseproxy.xensolution.com/connect-h2?host=backend.app"
  },
  "ReverseProxy": {
    "Routes": {
      "route1": {
        "ClusterId": "cluster1",
        "Match": {
          "Path": "/APIOne/{*remainder}"
          // "Path": "{**catch-all}"
        },
        "Transforms":[
          {
            "PathRemovePrefix":"/APIOne"
          }
        ]
      },
      "route2": {
        "ClusterId": "cluster2",
        "Match": {
          "Path": "/APITwo/{*remainder}"
        },
        "Transforms":[
          {
            "PathRemovePrefix":"/APITwo"
          }
        ]
      }
    },
    "Clusters": {
      "cluster1": {
        "Destinations": {
          "cluster1/destination1": {
            "Address": "http://localhost:5001/"
          }
        }
      },
      "cluster2": {
        "Destinations": {
          "cluster2/destination1": {
            "Address": "http://localhost:5002/"
          }
        }
      }
    }
  }
}
